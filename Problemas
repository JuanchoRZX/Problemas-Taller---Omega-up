def filtrarNombres(nombres):
    NombresD = []
    for nombre in nombres:
        if nombre[0] == 'E':
            NombresD.append(nombre)
    return NombresD
nombres = []
for _ in range(N):
    nombre = input()
    nombres.append(nombre)
for i in filtrarNombres(nombres):
    print(i)
    

42. 16501. Posición del Jugador

N = int(input())
numeros = []
for _ in range(N):
    numero = int(input())
    numeros.append(numero)
def obtposicion(N, numeros):
    posiciones = []
    for i in range(N):
        posiciones.append(numeros[i])
    return posiciones
while True:
    numero = int(input())
    if numero == -1:break
    print(obtposicion(N, numeros)[numero])
   
